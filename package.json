{
  "name": "sayt",
  "version": "1.0.0",
  "description": "Prototypes for both Search and Sayt.",
  "main": "index.html",
  "dependencies": {
    "acorn": "^6.1.1",
    "http-server": "^0.11.1",
    "lit-html": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "@open-wc/testing-karma": "^2.0.0",
    "webpack-merge": "^4.1.5",
    "@open-wc/testing-karma-bs": "^1.0.0",
    "@open-wc/testing": "^0.11.1",
    "@open-wc/demoing-storybook": "^0.2.0"
  },
  "scripts": {
    "test": "karma start --coverage",
    "tdd": "npm test -- --watch",
    "start": "http-server",
    "watch": "npx webpack --config webpack.config.js --watch",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "test:legacy": "karma start --legacy --coverage",
    "test:legacy:watch": "karma start --legacy --auto-watch=true --single-run=false",
    "test:bs": "karma start karma.bs.config.js --legacy --coverage",
    "site:build": "npm run storybook:build",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook -o _site -s storybook-static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielMoniz-GBI/search_and_sayt_prototype.git"
  },
  "author": "Daniel Moniz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanielMoniz-GBI/search_and_sayt_prototype/issues"
  },
  "jest": {
    "transformIgnorePatterns": [
      "!node_modules"
    ]
  },
  "homepage": "https://github.com/DanielMoniz-GBI/search_and_sayt_prototype#readme"
}
